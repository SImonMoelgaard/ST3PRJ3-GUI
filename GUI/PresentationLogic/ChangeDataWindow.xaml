        <Button x:Name="Next_B" Content="Færdig" Margin="241,203,11,0" VerticalAlignment="Top"  Background="#FF5B9BD5" Grid.ColumnSpan="3" Height="20" Click="Next_B_Click"/>
        <Label Content="Middel:" HorizontalAlignment="Left" Margin="4,137,0,0" VerticalAlignment="Top" Width="67" Grid.ColumnSpan="3" Height="26"/>
        <TextBox x:Name="sysULimit_TB" HorizontalAlignment="Left" Margin="79,65,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="86" Height="29" Text="140" Grid.ColumnSpan="3"/>
        <TextBox x:Name="diaULimit_TB" HorizontalAlignment="Left" Margin="79,100,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="86" Height="29" Text="90" Grid.ColumnSpan="3"/>
        <TextBox x:Name="sysLLimit_TB" HorizontalAlignment="Left" Margin="170,65,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="86" Height="29" Text="100" Grid.ColumnSpan="3"/>
        <TextBox x:Name="diaLLimit_TB" HorizontalAlignment="Left" Margin="170,100,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="86" Height="29" Text="60" Grid.ColumnSpan="3"/>
        <TextBox x:Name="meanULimit_TB" HorizontalAlignment="Left" Margin="79,137,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="86" Height="29" Text="100" Grid.ColumnSpan="3"/>
        <TextBox x:Name="meanLLimit_TB" HorizontalAlignment="Left" Margin="170,137,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="86" Height="29" Text="90" Grid.ColumnSpan="3"/>
        <Button x:Name="ExitToMainWindow_B" Content="Luk" Margin="241,14,11,0" VerticalAlignment="Top" Width="76" Click="ExitToMainWindow_B_Click" Background="#FF5B9BD5" Grid.ColumnSpan="3"/>

