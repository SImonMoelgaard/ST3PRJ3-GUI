<Window x:Class="PresentationLogic.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PresentationLogic"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Margin="0,0,0,7">
        <Button x:Name="PerformMeasurement_B" Content="Foretag Måling" HorizontalAlignment="Left" Margin="30,92,0,0" VerticalAlignment="Top" Height="44" Width="90" Click="PerformMeasurement_B_Click"/>
        <Button x:Name="PerfromCalibration_B" Content="Foretag Kalibrering" HorizontalAlignment="Left" Margin="158,93,0,0" VerticalAlignment="Top" Height="43" Width="122" Click="PerfromCalibration_B_Click"/>
        <Button x:Name="Exit_B" Content="Luk Program" HorizontalAlignment="Left" Margin="192,14,0,0" VerticalAlignment="Top" Height="36" Width="88" Click="Exit_B_Click"/>
        <RadioButton x:Name="English_RB" Content="Engelsk" HorizontalAlignment="Left" Margin="30,25,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
